generateDiagData <- function(n=100, ...){
x1 <- sample.int(n,n, replace=T)
x2 <- sample.int(n,n, replace=T)
lab <- x1<x2
x1 <- jitter(x1, ...)
x2 <- jitter(x2, ...)
data.frame(x1,x2,lab)
}
data <- generateDiagData(150, factor=100)
